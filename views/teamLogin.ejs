<% include ./partials/header %>
<div class="row">
    <div class="col-md-12"style="background-image: url('/images/bg-01.jpg');">
    <div class="wrap-login1000 p-t-30 p-b-50">
				<span class="login100-form-title p-b-41">
					Team Login
				</span>
    
 <form class="form-signin" action="/team/login" method="post">

            <h1 class="h3 mb-3 font-weight-normal">Team sign in</h1>
            
<div class="wrap-input100 validate-input" data-validate = "Enter username">
						<input class="input1000" type="text" name="username" placeholder="User name" required>
						<span class="focus-input1000" data-placeholder="&#xe82a;"></span>

					</div>

					<div class="wrap-input1000 validate-input" data-validate="Enter password">
						<input class="input1000" type="password" name="password" placeholder="Password" required>
						<span class="focus-input1000" data-placeholder="&#xe80f;"></span>
					</div>
<div class="container-login1000-form-btn m-t-32">
						<button class="login1000-form-btn" type="submit">
							Login
						</button>
					</div>           
        </form>
    </div>
    </div>
</div>
<% include ./partials/footer %>